{
  "name": "backend-segep",
  "version": "1.0.0",
  "description": "backend-template",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "rm -rf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "test:e2e": "NODE_ENV=test jest --runInBand -c jest.e2e.config.ts",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./src/_infra/database/typeorm",
    "migration:generate": "npm run typeorm migration:generate ./src/_infra/database/typeorm/migrations/$npm_config_name",
    "migration:generate:run": "npm run typeorm migration:generate ./src/_infra/database/typeorm/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "seeds": "ts-node -r tsconfig-paths/register ./src/_infra/database/typeorm/seeders/run-seeds.ts",
    "seeds:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./src/_infra/database/typeorm/seeders/run-seeds.ts",
    "seeds:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm-seeding -d src/_infra/database/typeorm seed ./src/_infra/database/typeorm/seeders/*.ts",
    "postmigration:generate:run": "npm run migration:run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "generate:docs": "typedoc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@jest/globals": "29.5.0",
    "@jorgebodega/typeorm-seeding": "6.0.1",
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/latinize": "0.2.17",
    "@types/node": "18.14.1",
    "@types/node-schedule": "2.1.4",
    "@types/pdfmake": "0.2.8",
    "@types/supertest": "2.0.12",
    "babel-jest": "29.5.0",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "esbuild": "0.17.18",
    "esbuild-jest": "0.5.0",
    "eslint": "8.34.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.12.3",
    "typedoc": "0.25.7",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@fastify/busboy": "2.0.0",
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.3.0",
    "@fastify/multipart": "8.0.0",
    "@fastify/static": "6.11.2",
    "axios": "1.6.0",
    "bcryptjs": "2.4.3",
    "date-fns": "3.3.1",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.13.0",
    "fastify-multer": "2.0.3",
    "jsonwebtoken": "9.0.1",
    "node-schedule": "2.1.1",
    "pdf-lib": "1.17.1",
    "pdfmake": "0.2.8",
    "pg": "8.10.0",
    "reflect-metadata": "0.1.13",
    "remove-accents": "0.5.0",
    "sharp": "0.32.6",
    "typeorm": "0.3.20",
    "zod": "3.20.6"
  }
}
